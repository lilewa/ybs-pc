        <parameter name="p_djh" type="Char" direction="in"></parameter>
        <parameter name="p_isbn" type="Char" direction="in"></parameter> 
        <commandType>Text</commandType>
        <sql>
          Select trim(t.isbn) isbn,
          sm 书名,
          dj 定价,t.id
